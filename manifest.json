{
    "manifest_version": 2,
    "name": "Translate Launcher",
    "description": "Launch Google-translate wits hotkey",

    "version": "1.0.0",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    }
  }